{
  "name": "typescript-tslint-plugin",
  "version": "0.0.1",
  "description": "TypeScript tslint language service plugin",
  "main": "out/index.js",
  "author": "Microsoft",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/typescript-tslint-plugin.git"
  },
  "scripts": {
    "prepublish": "(cd tslint-runner && npm run prepublish) && npm run compile",
