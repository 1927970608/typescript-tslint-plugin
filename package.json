    "test": "(cd tslint-runner && npm run test)",
    "lint": "tslint src/index.ts"
  },
  "dependencies": {
    "typescript-tslint-runner": "file:./tslint-runner"
  },
  "devDependencies": {
    "@types/node": "^8.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "peerDependencies": {
    "typescript": ">= 2.3.1",
    "tslint": ">= 4 < 6"
  }
}
