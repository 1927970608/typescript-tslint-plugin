{
  "name": "typescript-tslint-plugin",
  "version": "0.0.1",
  "description": "TypeScript tslint language service plugin",
  "main": "out/index.js",
  "author": "Microsoft",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/typescript-tslint-plugin.git"
  },
  "scripts": {
    "prepublish": "(cd tslint-runner && npm run prepublish) && node node_modules/typescript/lib/tsc && ",
    "compile": "(cd tslint-runner && npm run compile) && node node_modules/typescript/lib/tsc",
    "test": "(cd tslint-runner && npm run test)",
    "lint": "tslint src/index.ts"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "^7.0.8",
    "tslint": "^5.2.0",
    "typescript": "^3.0.1" 
  },
  "peerDependencies": {
    "typescript": ">= 2.3.1",
    "tslint": ">= 4 < 6"
  }
}
